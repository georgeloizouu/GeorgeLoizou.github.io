{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "VeChain Price",

   "title": {
    "text": "VeChain: trading range this month",
    "subtitle":"High-low range, 8h time periods. Source: coinapi.io",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start"},
  
   
  "data": {
      "url": "https://rest.coinapi.io/v1/ohlcv/VET/USD/history?period_id=8HRS&time_start=2021-10-10T00:00:00&limit=100&include_empty_items=false&apikey=96F17CD1-7520-40B1-B729-1507D00D0DB4",
      "format": {"type": "json"}},
  

  "height": 300,
  "width": 300,
  

  "mark": {
    "type": "area",
    "point": false,
    "color": "#e4c723"
  },

  
  "encoding": {
    
    "x": {
      "field": "time_period_start",
      "type": "temporal",
      "axis": {
        "title": null,
        "grid": false,
        "labelSeparation": 10}},

    "y": {
      "field": "price_low",
      "type": "quantitative",
      "title": "",
      "scale":{"domain":[0, 0.21]},
      "axis": {
        "grid": false,
        "format":"$"}},

    "y2": {"field": "price_high"}       
  }
}
