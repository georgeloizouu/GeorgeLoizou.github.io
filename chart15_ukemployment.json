{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "UK: Employment rate",
    "subtitle": "Data from 2000-2020 Source: statista.com",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 11,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/georgeloizouu/GeorgeLoizou.github.io/main/UK%23employmentrate.csv",
    "format": {"type": "csv"}
  },
  "height": 300,
  "width": 300,
  "mark": {"type": "line","color":"orangered", "point": false, "strokeWidth": 2, "opacity": 1},
  "encoding": {
    "x": {
      "field": "Date",
      "type": "temporal",
      "title": null,
      "axis": {"grid": true}
    },
    "y": {
      "field": "Employment rate",
      "type": "quantitative",
      "title": "Employment rate %",
      "axis": {"grid": true},
      "scale": {"domain": [60, 80]}
    }
  },
  "config": {}
}
